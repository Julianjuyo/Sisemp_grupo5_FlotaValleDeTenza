<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
    <ViewSettingsDialog>
        
        <filterItems>
            
            <ViewSettingsCustomItem text="fecha"
                                    filterCount="{parts:[{path:'/fecha/vValueLT'}, {path:'/fecha/vValueGT'}], formatter:'.filterCountFormatter'}">
                <customControl>
                    <l:VerticalLayout width="100%">
                        <Toolbar>
                            <Label width="25%" text="Before" class="sapUiSmallMarginBegin"/>
                            <DatePicker width="33%" value="{path: '/fecha/vValueLT'}"
                                        valueFormat="yyyy-MM-dd" displayFormat="yyyy-MM-dd"/>
                        </Toolbar>
                        <Toolbar>
                            <Label width="25%" text="After" class="sapUiSmallMarginBegin"/>
                            <DatePicker width="33%" value="{path: '/fecha/vValueGT'}"
                                        valueFormat="yyyy-MM-dd" displayFormat="yyyy-MM-dd"/>
                        </Toolbar>
                    </l:VerticalLayout>
                </customControl>
            </ViewSettingsCustomItem>
            
            
            
            <ViewSettingsCustomItem text="distancia"
                                    filterCount="{parts:[{path:'/distancia/vValueLT'}, {path:'/distancia/vValueGT'}], formatter:'.filterCountFormatter'}">
                <customControl>
                    <l:VerticalLayout width="100%">
                        <Toolbar>
                            <Label width="25%" text="Less than" class="sapUiSmallMarginBegin"/>
                            <Input width="33%" value="{path: '/distancia/vValueLT'}"/>
                        </Toolbar>
                        <Toolbar>
                            <Label width="25%" text="More than" class="sapUiSmallMarginBegin"/>
                            <Input width="33%" value="{path: '/distancia/vValueGT'}"/>
                        </Toolbar>
                    </l:VerticalLayout>
                </customControl>
            </ViewSettingsCustomItem>
            
            
            <ViewSettingsCustomItem text="tiempo"
                                    filterCount="{parts:[{path:'/tiempo/vValueLT'}, {path:'/tiempo/vValueGT'}], formatter:'.filterCountFormatter'}">
                <customControl>
                    <l:VerticalLayout width="100%">
                        <Toolbar>
                            <Label width="25%" text="Less than" class="sapUiSmallMarginBegin"/>
                            <Input width="33%" value="{path: '/tiempo/vValueLT'}"/>
                        </Toolbar>
                        <Toolbar>
                            <Label width="25%" text="More than" class="sapUiSmallMarginBegin"/>
                            <Input width="33%" value="{path: '/tiempo/vValueGT'}"/>
                        </Toolbar>
                    </l:VerticalLayout>
                </customControl>
            </ViewSettingsCustomItem>
            
            
            <ViewSettingsCustomItem text="tarifa"
                                    filterCount="{parts:[{path:'/tarifa/vValueLT'}, {path:'/tarifa/vValueGT'}], formatter:'.filterCountFormatter'}">
                <customControl>
                    <l:VerticalLayout width="100%">
                        <Toolbar>
                            <Label width="25%" text="Less than" class="sapUiSmallMarginBegin"/>
                            <Input width="33%" value="{path: '/tarifa/vValueLT'}"/>
                        </Toolbar>
                        <Toolbar>
                            <Label width="25%" text="More than" class="sapUiSmallMarginBegin"/>
                            <Input width="33%" value="{path: '/tarifa/vValueGT'}"/>
                        </Toolbar>
                    </l:VerticalLayout>
                </customControl>
            </ViewSettingsCustomItem>
            
            
        </filterItems>
        

        

        
    </ViewSettingsDialog>
</core:FragmentDefinition>
